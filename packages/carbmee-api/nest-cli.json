{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/carbmee-api/src",
  "monorepo": true,
  "root": "apps/carbmee-api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/carbmee-api/tsconfig.app.json"
  },
  "projects": {
    "carbmee-api": {
      "type": "application",
